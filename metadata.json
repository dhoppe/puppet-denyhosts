{
  "name": "dhoppe-denyhosts",
  "version": "1.0.6",
  "author": "Dennis Hoppe",
  "summary": "This module installs, configures and manages the Denyhosts service.",
  "license": "Apache-2.0",
  "source": "git://github.com/dhoppe/puppet-denyhosts.git",
  "project_page": "https://github.com/dhoppe/puppet-denyhosts",
